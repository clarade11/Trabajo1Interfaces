<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AgendaDeContactosNuevoController">
   <center>
      <AnchorPane prefHeight="400.0" prefWidth="567.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane layoutX="11.0" layoutY="11.0" prefHeight="289.0" prefWidth="544.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <TextField fx:id="tfNombre" promptText="Ej: Pepe" GridPane.columnIndex="1" />
                  <TextField fx:id="tfApellido" promptText="Ej: Coco" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="tfCalle" promptText="Ej: Calle Alameda" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="tfCiudad" promptText="Ej: Ciudad" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="tfCodigoPostal" promptText="Ej: 41003" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <TextField fx:id="tfFecha" promptText="dd/mm/yyyy" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <TextField fx:id="tfTelefono" promptText="Ej: 321654987" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <Label text="Nombre" />
                  <Label text="Apellido" GridPane.rowIndex="1" />
                  <Label text="Calle" GridPane.rowIndex="2" />
                  <Label text="Ciudad" GridPane.rowIndex="3" />
                  <Label text="Codigo Postal" GridPane.rowIndex="4" />
                  <Label text="Fecha de nacimiento" GridPane.rowIndex="5" />
                  <Label text="Telefono" GridPane.rowIndex="6" />
               </children>
            </GridPane>
            <Button fx:id="botonCancelar" layoutX="450.0" layoutY="352.0" mnemonicParsing="false" onAction="#cancelar" prefHeight="26.0" prefWidth="91.0" text="Cancelar" />
            <Button fx:id="botonAnadir" layoutX="338.0" layoutY="352.0" mnemonicParsing="false" onAction="#volver" prefHeight="26.0" prefWidth="91.0" text="Listo" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
